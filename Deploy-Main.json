{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "deploychoice": {
      "type": "string",
      "metadata": {
        "description": "Environment to Deploy"
      },
      "allowedValues": [
        "https://multideploydemo.blob.core.windows.net/pubtest/cust1-template-network.json",
        "https://multideploydemo.blob.core.windows.net/pubtest/cust1-template-domaincontrollers_withkey.json",
        "https://multideploydemo.blob.core.windows.net/pubtest/cust1-template-multisvrdeploy_withkey.cse.json",
        "https://raw.githubusercontent.com/boklyn/Cust1Testing/master/cust1-apptier.twosvrdeploy.json"
      ]
    },
    "paramchoice": {
      "type": "string",
      "metadata": {
        "description": "Different Parameters files choice"
      },
      "allowedValues": [
        "https://multideploydemo.blob.core.windows.net/pubtest/cust1-parameters-network.json",
        "https://multideploydemo.blob.core.windows.net/pubtest/cust1-parameters-key.domaincontrollers.json",
        "https://multideploydemo.blob.core.windows.net/pubtest/cust1-parameters-key.admintier.svrs.json",
        "https://raw.githubusercontent.com/boklyn/Cust1Testing/master/cust1-webtier.parameters.json",
        "https://raw.githubusercontent.com/boklyn/Cust1Testing/master/cust1-apptier.appsvrs.parameters.json",
        "https://raw.githubusercontent.com/boklyn/Cust1Testing/master/cust1-apptier.edwvrs.parameters.json",
        "https://raw.githubusercontent.com/boklyn/Cust1Testing/master/cust1-apptier.rulessvrs.parameters.json"
      ]
    }

  },
  "resources": [
    {
      "apiVersion": "2015-01-01",
      "name": "ProductionDeployment",
      "type": "Microsoft.Resources/deployments",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[parameters('deploychoice')]",
          "contentVersion": "1.0.0.0"
        },
        "parametersLink": {
          "uri": "[parameters('paramchoice')]",
          "contentVersion": "1.0.0.0"
        }
      }
    }
  ]
}